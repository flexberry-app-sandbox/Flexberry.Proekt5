<h3 class="ui header">{{t "forms.i-i-s-proekt-формир-рез-пос-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-формир-рез-пос-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-proekt-формир-рез-пос-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.номерДокРаспр
      class=(if (v-get validationModel.формирРаспред "номерДокРаспр" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.номерДокРаспр-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-номерДокРаспр=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред "номерДокРаспр" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.докумПостав.организация.наименование
      class=(if (v-get validationModel.формирРаспред.докумПостав.организация "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.докумПостав.организация.наименование-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-докумПостав-организация-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред.докумПостав.организация "наименование" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.докумПостав.клиенты.наимЗаказ
      class=(if (v-get validationModel.формирРаспред.докумПостав.клиенты "наимЗаказ" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.докумПостав.клиенты.наимЗаказ-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-докумПостав-клиенты-наимЗаказ=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред.докумПостав.клиенты "наимЗаказ" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.докумПостав.клиенты.телефон
      class=(if (v-get validationModel.формирРаспред.докумПостав.клиенты "телефон" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.докумПостав.клиенты.телефон-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-докумПостав-клиенты-телефон=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред.докумПостав.клиенты "телефон" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.пунктПогрузки.наименование
      class=(if (v-get validationModel.формирРаспред.пунктПогрузки "наименование" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.пунктПогрузки.наименование-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-пунктПогрузки-наименование=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред.пунктПогрузки "наименование" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.формирРаспред.докумПостав.клиенты.адресПостав
      class=(if (v-get validationModel.формирРаспред.докумПостав.клиенты "адресПостав" "isInvalid") "error")
      label=(t "forms.i-i-s-proekt-формир-рез-пос-e.формирРаспред.докумПостав.клиенты.адресПостав-caption")
      data-test-i-i-s-proekt-формир-рез-пос-e-формирРаспред-докумПостав-клиенты-адресПостав=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.формирРаспред.докумПостав.клиенты "адресПостав" "message")}}
  </div>
  <div class="field" data-test-i-i-s-proekt-формир-рез-пос-e-статусЗаказа="true">
    <label>{{t "forms.i-i-s-proekt-формир-рез-пос-e.статусЗаказа-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-proekt-статус-заказа")
      value=model.статусЗаказа
      class=(if (v-get validationModel "статусЗаказа" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "статусЗаказа" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proekt-формир-рез-пос-e.наличиеДеф-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.наличиеДеф
      class=(if (v-get validationModel "наличиеДеф" "isInvalid") "error")
      data-test-i-i-s-proekt-формир-рез-пос-e-наличиеДеф=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "наличиеДеф" "message")}}
  </div>
  <div class="field" data-test-i-i-s-proekt-формир-рез-пос-e-удовлКл="true">
    <label>{{t "forms.i-i-s-proekt-формир-рез-пос-e.удовлКл-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-proekt-удовл-кл")
      value=model.удовлКл
      class=(if (v-get validationModel "удовлКл" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "удовлКл" "message")}}
  </div>
</form>
